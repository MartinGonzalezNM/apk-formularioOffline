{
  "expo": {
    "name": "FormularioOffline",
    "slug": "FormularioOffline",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.martin7304.FormularioOffline"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicación necesita acceso a tus fotos",
          "cameraPermission": "La aplicación necesita acceso a tu cámara"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5d95cddd-2f57-4d00-8463-117ee886f211"
      }
    }
  }
}
